{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "sg2o.user.js",
                              "jquery/jquery-2.1.3.min.js",
                              "jquery/jquery-ui-1.11.4/jquery-ui.min.js",
                              "jquery/jquery-ui-slider-pips.min.js"
                            ],
        "css":              [ "jquery/jquery-ui-slider-pips.css"
                            ],
        "matches":          [   "http://www.steamgifts.com/*"
                            ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "Bring old functions from sg+ to SGv2.",
    "name":         "sg2o",
    "version":      "0.2.9",
    "web_accessible_resources": [ "sg2o.user.js.map",
                                  "lib/*.dart",
                                  "bin/*.dart"
                                ]
}
